{
    "tracker_default": {
        "type": "container",
        "background": "#FF99ccff",
        "content": {
            "type": "dock",
            "dropshadow": true,
            "content": [
                {
                    "type": "dock",
                    "dock": "left",
                    "v_alignment": "stretch",
                    "content": [
                        {
                            "type": "group",
                            "header": "Items",
                            "dock": "top",
                            "content": {
                                "type": "layout",
                                "key": "shared_item_grid_vertical"
                            }
                        }
                    ]
                }
            ]
        }
    },
    "tracker_horizontal": {
        "type": "container",
        "background": "#FF99ccff",
        "content": {
            "type": "dock",
            "dropshadow": true,
            "content": [
                {
                    "type": "dock",
                    "dock": "bottom",
                    "content": [
                        {
                            "type": "group",
                            "header": "Items",
                            "dock": "left",
                            "margin": "0,0,3,0",
                            "content": {
                                "type": "layout",
                                "h_alignment": "center",
                                "v_alignment": "center",
                                "key": "shared_item_grid_horizontal"
                            }
                        }
                    ]
                },
            ]
        }
    }
